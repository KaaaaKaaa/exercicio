function readArrayA() {
  const arrayA = [];
  for (let i = 0; i < 10; i++) {
    const number = parseFloat(prompt(`Digite o nÃºmero ${i + 1}:`));
    arrayA.push(number);
  }
  return arrayA;
}

function main() {
  const arrayA = readArrayA();
  const X = parseFloat(prompt("Digite o valor de X:"));
  const arrayM = arrayA.map((number) => number * X);
  console.log("Array M:", arrayM);
}
main();
